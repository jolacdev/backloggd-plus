    "@testing-library/react": "16.3.0",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "@vitest/browser": "4.0.17",

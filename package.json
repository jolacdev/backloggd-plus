{
  "name": "backloggd-plus",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "tsc && wxt build",
    "build:firefox": "tsc && wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "tscheck": "tsc --noEmit",
    "lint": "pnpm tscheck && eslint --fix",
    "lint:errors": "pnpm tscheck && eslint --fix --quiet",
    "lint:no-fix": "pnpm tscheck && eslint"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.16",
